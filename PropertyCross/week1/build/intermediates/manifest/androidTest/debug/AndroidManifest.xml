<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lasalle.second.part.week1.test" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:functionalTest="false"
        android:handleProfiling="false"
        android:label="Tests for com.lasalle.second.part.week1.test"
        android:targetPackage="com.lasalle.second.part.week1.test" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.lasalle.second.part.week1.PropertyCrossApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <uses-library android:name="android.test.runner" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity android:name="com.lasalle.second.part.week1.activities.MainActivity" />
        <activity android:name="com.lasalle.second.part.week1.activities.ResultsContainerActivity" />
        <activity android:name="com.lasalle.second.part.week1.activities.UserLoginActivity" />
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
    </application>

</manifest>